<?xml version="1.0" encoding="UTF-8"?>
<groups xmlns="http://www.isdc.ro/wro"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.isdc.ro/wro wro.xsd">

    <group name="styles">
        <css>/css/bootstrap.min.css</css>
        <css>/css/bootstrap-responsive.min.css</css>
        <css>/css/default.*.css</css>
    </group>

    <group name="admin.styles">
        <css>/css/bootstrap.min.css</css>
        <css>/css/admin.*.css</css>
    </group>

    <group name="libs">
        <js minimize="false">/js/lib/jquery/*.js</js>
        <js minimize="false">/js/lib/bootstrap/*.js</js>
        <js minimize="false">/js/lib/nicescoller/*.js</js>
        <js minimize="false">/js/lib/angular/angular.min.js</js>
        <js minimize="false">/js/lib/angular/angular-*.js</js>
        <js minimize="false">/js/lib/angularui/*.js</js>
        <js minimize="false">/js/lib/*.js</js>
    </group>

    <group name="scripts">
        <js>/WEB-INF/coffee/default.*.coffee</js>
    </group>

    <group name="admin.scripts">
        <js>/WEB-INF/coffee/admin.*.coffee</js>
    </group>

</groups>