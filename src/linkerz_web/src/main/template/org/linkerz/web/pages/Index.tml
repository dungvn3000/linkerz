<html t:type="layout" title="LinkerZ Index"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">

    <ul class="nav">
        <t:loop source="result" value="link">
            <li>
                <a href="${link.url}">${link.title}</a>
                <p>
                    ${link.html.subString(0, 200)}
                </p>
            </li>
        </t:loop>
    </ul>

</html>
