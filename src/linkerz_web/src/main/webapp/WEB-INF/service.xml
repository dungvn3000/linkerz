<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="org.linkerz.web.services.db.DBStoreImpl">
        <property name="mongoOperations" ref="mongoTemplate"/>
    </bean>

    <bean class="org.linkerz.web.services.user.UserServiceImpl">
        <property name="mongoOperations" ref="mongoTemplate"/>
    </bean>

    <bean class="org.linkerz.web.services.plugin.PluginServiceImpl">
        <property name="mongoOperations" ref="mongoTemplate"/>
    </bean>

</beans>